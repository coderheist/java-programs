package array;
import java.util.Scanner;
public class search_element {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int sum=0;
		int b=0;
		int []arr ;
		int []arr1;
		arr= new int[n];
		for(int j=0;j<n;j++) {
			int a=sc.nextInt();
			arr[j]=a;}
		System.out.print(" enter the number to the searched : ");
		int x=sc.nextInt();
		for(int i=0;i<n;i++) {
			if(arr[i]==n) {
				System.out.println(i);
			}
		}
	}
}
