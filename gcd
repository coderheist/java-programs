import java.util.*;
public class Main {
	public static int GCD(int n1,int n2){
		int gcd=Math.min(n1,n2);
		while(gcd!=0){
			if(n1%gcd==0 && n2%gcd==0){
				break;
			}
			gcd--;
		}return gcd;
	}
    public static void main(String args[]) {
		Scanner sc=new Scanner(System.in);
		int n1=sc.nextInt();
		int n2=sc.nextInt();
		System.out.println(GCD(n1,n2));


    }
}
