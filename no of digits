package LOOP;
import java.util.*;
public class noofdigit {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int temp=sc.nextInt();
		int count=0;
		int last=0;
		
		while(temp>0)   
		{         
		last = temp % 10;          
		temp = temp/10;
		count=count+1;
		}    
		System.out.println(count);
}}
