package LOOP;
import java.util.*;
public class sumofdigit {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int temp=sc.nextInt();
		int sum=0;
		int last=0;
		while(temp>0)   
		{         
		last = temp % 10;     
		sum = sum+last;      
		temp = temp/10;   
		}    
		System.out.print(sum);
	}

}
