import java.util.*;
public class Main {
	public static int LCM(int n1,int n2){
		int lcm=Math.max(n1,n2);
		while(lcm!=0){
			if(lcm%n1==0 && lcm%n2==0){
				break;
			}
			++lcm;
		}return lcm;
	}
    public static void main(String args[]) {
			Scanner sc=new Scanner(System.in);
			int n1=sc.nextInt();
			int n2=sc.nextInt();
			System.out.println(LCM(n1,n2));

    }
}
