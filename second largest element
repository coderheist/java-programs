package array;
import java.util.Scanner;
public class second_largest {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int sum=0;
		int res=0;
		int []arr ;
		int []arr1;
		arr= new int[n];
		for(int j=0;j<n;j++) {
			int a=sc.nextInt();
			arr[j]=a;
		}
		 for (int i = 1; i < n-1; i++)
	            res = Math.min(arr[i], arr[i+1]);
		System.out.println(" The second largest element is "+res);

	}

}
