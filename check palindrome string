import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String s=sc.next();
        String s1="";
        for(int i=s.length()-1;i>=0;i--){
            s1+=s.charAt(i);
        }
        if(s1.compareTo(s)==0){
            System.out.println("true");
        }
        else{
            System.out.println("false");
        }
    }
}
