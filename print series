import java.util.*;
public class Main {
    public static void main(String args[]) {
		Scanner sc=new Scanner(System.in);
		int N1=sc.nextInt();
		int N2=sc.nextInt();
		int count=0;
		for(int n=1;n<100;n++){
			int num= 3*n+2;
			if(num%N2!=0){
				System.out.println(num);
				count++;
			}
			if(count==N1){
				break;
			}
		}
		
    }
}
