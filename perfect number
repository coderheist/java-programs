package LOOP;
import java.util.*;
public class perfect_no {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int count=0;
		for(int i=1;i<n;i++) {
			if(n%i==0) {
				count=count+i;}
		}
		if(n==count) {
			System.out.println("Perfect number");
		}
		else {
			System.out.println("Non perfect number");
		}
}}
