package String;
import java.util.Scanner;
public class string_compression {

	public static void main(String[] args) {
		 Scanner sc=new Scanner(System.in);
	        String str=sc.next();
	        int [] b= new int[str.length()];
	        String str1=" ";
	        for(int i=0;i<str.length();i++){
	            b[str.charAt(i)-'a']++;
	            str1=str1+b[i];
	        }
	        System.out.println(str1);
	        

	}

}
